<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
<script src="/js/main.js" type="application/javascript"></script>
<% if (js_file.length !== 0) { %>
    <% for (let i = 0; i < js_file.length; i++) { %>
        <script src="/js/<%= js_file[i] %>.js" type="application/javascript"></script>
    <% } %>
<% } %>
<%
let alert = request.session[SessionVariableDictionary.ALERT];
if (alert !== undefined) {
%>
    <script>
        alert("<%= alert %>")
    </script>
<%
    request.session[SessionVariableDictionary.ALERT] = undefined;
}
%>